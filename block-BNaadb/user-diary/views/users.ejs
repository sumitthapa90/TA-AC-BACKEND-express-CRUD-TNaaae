<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users</title>
  </head>
  <body>
    <h1>List Of Users</h1>
    <% users.forEach(user => { %>
    <a href="/users/<%= user.id %>">
      <h3><%= user.name %></h3>
    </a>
    <!-- <p><%= user.email %></p>
        <p><%=  user.age %></p> -->
    <% }) %>
  </body>
</html>
