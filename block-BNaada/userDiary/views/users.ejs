<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <div class="store flex">
      <a href="/">Bookstore</a>
      <h3>User List</h3>
      <a href="/users/new">Add User</a>
    </div>
    <div class="section1">
      <div>
        <% users.forEach(user => { %>
        <h2><%= user.name %></h2>
        <p><%= user.username %></p>
        <h3><%= user.email %></h3>
        <p><%= user.age %></p>
        <% }) %>
      </div>
      <a href="/users/editUserForm">Edit</a>
    </div>
  </body>
</html>
